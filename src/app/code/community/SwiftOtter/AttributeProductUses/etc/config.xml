<config>
    <modules>
        <SwiftOtter_AttributeProductUses>
            <version>1</version>
        </SwiftOtter_AttributeProductUses>
    </modules>

    <global>
        <helpers>
            <SwiftOtter_AttributeProductUses>
                <class>SwiftOtter_AttributeProductUses_Helper</class>
            </SwiftOtter_AttributeProductUses>
        </helpers>

        <blocks>
            <SwiftOtter_AttributeProductUses>
                <class>SwiftOtter_AttributeProductUses_Block</class>
            </SwiftOtter_AttributeProductUses>
        </blocks>

        <models>
            <SwiftOtter_AttributeProductUses>
                <class>SwiftOtter_AttributeProductUses_Model</class>
