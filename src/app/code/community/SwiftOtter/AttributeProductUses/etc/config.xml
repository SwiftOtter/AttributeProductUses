            </SwiftOtter_AttributeProductUses>
            <SwiftOtter_AttributeProductUses_Resource>
                <class>SwiftOtter_AttributeProductUses_Model_Resource</class>
            </SwiftOtter_AttributeProductUses_Resource>
            <eav_resource>
                <rewrite>
                    <entity_attribute_option_collection>SwiftOtter_AttributeProductUses_Model_Resource_Eav_Entity_Attribute_Option_Collection</entity_attribute_option_collection>
                </rewrite>
            </eav_resource>
        </models>

        <resources>
