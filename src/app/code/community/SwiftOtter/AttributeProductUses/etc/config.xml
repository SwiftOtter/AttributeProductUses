                    <class>Mage_Eav_Model_Entity_Setup</class>
                </setup>
            </SwiftOtter_AttributeProductUses>
        </resources>
    </global>

    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <SwiftOtter_AttributeProductUses extends="Mage_Adminhtml">SwiftOtter_AttributeProductUses</SwiftOtter_AttributeProductUses>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>

    <adminhtml>
        <layout>
            <updates>
                <SwiftOtter_AttributeProductUses>
                    <file>SwiftOtter/Attribute.xml</file>
                </SwiftOtter_AttributeProductUses>
            </updates>
        </layout>
    </adminhtml>
</config>